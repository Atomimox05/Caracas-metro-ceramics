{
	"schemaVersion": 1,
	"id": "mccsj",
	"version": "${version}",
	"name": "Caracas metro ceramics",
	"description": "A minecraft mod that uses custom blocks with the ceramic designs of the Caracas Metro line 2.",
	"authors": [
		"Atomimox"
	],
	"contact": {
		"homepage": "https://codejod.com/",
		"sources": "https://github.com/Atomimox05/Caracas-metro-ceramics"
	},
	"license": "MIT",
	"icon": "assets/mccsj/icon.png",
