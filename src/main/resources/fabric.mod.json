	"entrypoints": {
		"main": [
			"com.ax5.mccsj.CaracasMetroCeramics"
		],
		"fabric-datagen": [
			"com.ax5.mccsj.CaracasMetroCeramicsDataGenerator"
		],
		"client": [
			"com.ax5.mccsj.CaracasMetroCeramicsClient"
		]
	},
	"mixins": [
		"mccsj.mixins.json"
